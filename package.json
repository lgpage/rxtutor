    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/cli": "^14.2.5",
    "@angular/compiler-cli": "^14.2.5",
    "@compodoc/compodoc": "^1.1.19",
    "@types/codemirror": "^5.60.5",
    "@types/jasmine": "^4.3.0",
    "@types/uuid": "^8.3.4",
