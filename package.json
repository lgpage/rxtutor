{
  "name": "rxtutor",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:stats": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "test:before-push": "npm run lint && npm run test:headless && npm run build:prod",
    "lint": "ng lint",
    "docs": "npx compodoc -p tsconfig.doc.json",
    "view:stats": "webpack-bundle-analyzer ./dist/nana-page/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.5",
