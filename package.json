{
  "name": "rxtutor",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:stats": "ng build --stats-json --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "test:before-push": "npm run lint && npm run test:headless && npm run build:prod",
    "lint": "ng lint",
    "docs": "npx compodoc -p tsconfig.doc.json",
    "view:stats": "webpack-bundle-analyzer ./dist/rxtutor/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.1",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.1",
    "@angular/compiler": "^15.0.1",
    "@angular/core": "^15.0.1",
    "@angular/forms": "^15.0.1",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.1",
    "@angular/platform-browser-dynamic": "^15.0.1",
    "@angular/router": "^15.0.1",
    "@ctrl/ngx-codemirror": "^5.1.1",
    "@microsoft/applicationinsights-angularplugin-js": "^3.0.0",
    "@microsoft/applicationinsights-web": "^2.8.9",
    "@ngneat/until-destroy": "^9.2.2",
    "bootstrap": "^5.2.2",
    "codemirror": "^5.65.10",
    "github-fork-ribbon-css": "~0.2.3",
    "rxjs": "^7.5.7",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.1",
    "@angular-eslint/builder": "^15.0.0",
    "@angular-eslint/eslint-plugin": "^15.0.0",
    "@angular-eslint/eslint-plugin-template": "^15.0.0",
    "@angular-eslint/schematics": "^15.0.0",
    "@angular-eslint/template-parser": "^15.0.0",
    "@angular/cli": "^15.0.1",
    "@angular/compiler-cli": "^15.0.1",
    "@compodoc/compodoc": "^1.1.19",
    "@types/codemirror": "^5.60.5",
    "@types/jasmine": "^4.3.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-plugin-rxjs": "^5.0.2",
    "jasmine-core": "^4.5.0",
    "jasmine-marbles": "~0.9.2",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-mocks": "^14.4.0",
    "typescript": "^4.8.4",
    "webpack-bundle-analyzer": "^4.7.0"
  }
}